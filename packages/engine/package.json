  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core.wasm": {
      "types": "./dist/generated/core_bg.wasm.d.ts",
      "import": "./dist/generated/core_bg.wasm"
