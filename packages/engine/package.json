{
  "name": "@graphql-steel/engine",
  "version": "0.0.10",
  "license": "MIT",
  "author": "oneofthezombies <hunhoekim@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "deno": {
        "types": "./dist/index-deno.d.ts",
        "import": "./dist/index-deno.js"
      },
      "node": {
        "types": "./dist/index-node.d.ts",
        "import": "./dist/index-node.js"
      },
      "workerd": {
        "types": "./dist/index-workerd.d.ts",
        "import": "./dist/index-workerd.js"
      },
      "edge-light": {
        "types": "./dist/index-edge-light.d.ts",
        "import": "./dist/index-edge-light.js"
      },
      "browser": {
        "types": "./dist/index-browser.d.ts",
        "import": "./dist/index-browser.js"
      },
      "default": {
        "types": "./dist/index-default.d.ts",
        "import": "./dist/index-default.js"
      }
    },
    "./engine.wasm": {
      "types": "./engine.wasm.d.ts",
      "default": "./engine.wasm"
    },
    "./engine.wasm?module": {
      "types": "./engine.wasm.d.ts",
      "default": "./engine.wasm?module"
    }
  },
  "scripts": {
    "build": "tsx ../../scripts/build-engine-js.ts &&  tsx scripts/update-package-version-ts.ts && vite build",
    "test": "vitest --run"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "engine.wasm",
    "engine.wasm.d.ts"
  ]
}
