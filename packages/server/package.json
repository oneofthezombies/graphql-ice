      "import": "./dist/adapter/vercel-functions-edge.js"
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "12.1.2",
    "esbuild": "0.24.2",
    "rollup": "4.29.1",
    "rollup-plugin-esbuild": "6.1.1"
