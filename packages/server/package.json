{
  "name": "@graphql-ice/server",
  "version": "0.1.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "./core.wasm": {
      "types": "./dist/generated/core.wasm.d.ts",
      "import": "./dist/generated/core.wasm"
    },
    "./adapter/cloudflare-workers": {
      "types": "./dist/adapter/cloudflare-workers.d.ts",
      "import": "./dist/adapter/cloudflare-workers.js"
    },
    "./adapter/deno-deploy": {
      "types": "./dist/adapter/deno-deploy.d.ts",
      "import": "./dist/adapter/deno-deploy.js"
